public class Test2 {
    public static void main(String[] arg) {
        
        int count = 0;

        int arrow[] = {5,4,6,-8,0,12,54,-321,6,2,1,0,478,3,10,-6,7,11,17,223,2,5,8};
        
        for (int i=0;i<arrow.length;i++) {
             if (arrow[i]>i){ 
                 System.out.print(arrow[i]+" ");
             }
        }
    }
}

