public class Test1 {
    public static void main(String[] arg) {
        int x1,x2,x3,y1,y2,y3;
        x1=1;
        x2=2;
        x3=3;
        y1=1;
        y2=2;
        y3=3;
        if ((x3 - x1) / (x2 - x1) == (y3 - y1) / (y2 - y1)){
            System.out.println("Точки лежат на 1ой прямой");
        }
        else System.out.println("Точки не лежат на 1ой прямой");
    }
}
